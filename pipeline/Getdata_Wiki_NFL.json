{
	"name": "Getdata_Wiki_NFL",
	"properties": {
		"description": "Get data for NFL annual scoring leaders by season",
		"activities": [
			{
				"name": "Copy_cwe",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "Source",
						"value": "/1"
					},
					{
						"name": "Destination",
						"value": "sun//NFL_annual_scoring_by_season"
					}
				],
				"typeProperties": {
					"source": {
						"type": "WebSource"
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false,
					"validateDataConsistency": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "SourceDataset_cwe",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "DestinationDataset_cwe",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}